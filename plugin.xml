<?xml version="1.0"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android"
        id="cordova-plugin-couchbase-lite-2.0" version="1.0.2">
  <name>Cordova Couchbase Lite 2.0</name>
  <description>Provides Typescript and ScalaJS APIs over the provided REST API.</description>
  <license>Apache 2.0</license>
  <js-module src="www/cblite.js" name="CouchbaseLite">
    <clobbers target="cbl"/>
  </js-module>
  <engines>
    <engine name="cordova" version=">=6.0.0"/>
  </engines>
  <platform name="ios">
    <config-file target="config.xml" parent="/widget">
      <feature name="CBLite">
        <param name="ios-package" value="CBLite"/>
        <param name="onload" value="true"/>
      </feature>
    </config-file>
    <header-file src="src/ios/CBLite.h"/>
    <source-file src="src/ios/CBLite.m"/>
    <framework src="libicucore.dylib"/>
    <framework src="SystemConfiguration.framework"/>
    <framework src="JavaScriptCore.framework"/>
    <framework custom="true" embed="true" src="lib/ios/CouchbaseLite.framework"/>
    <source-file src="lib/ios/CouchbaseLite.framework" target-dir="lib" framework="true" />
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLArray.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLArrayFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLAuthenticator.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLBasicAuthenticator.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLBlob.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDatabase.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDatabaseChange.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDatabaseConfiguration.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDatabaseEndpoint.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDictionary.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDictionaryFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDocument.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDocumentChange.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLDocumentFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLEndpoint.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLErrors.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLFullTextIndex.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLIndex.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLIndexBuilder.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLListenerToken.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLMutableArray.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLMutableArrayFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLMutableDictionary.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLMutableDictionaryFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLMutableDocument.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLMutableFragment.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQuery.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryArrayExpression.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryArrayFunction.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryBuilder.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryChange.h"/>
