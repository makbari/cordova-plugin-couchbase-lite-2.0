    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryLimit.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryMeta.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryOrdering.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryParameters.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryResult.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryResultSet.h"/>
    <header-file src="lib/ios/CouchbaseLite.framework/Headers/CBLQueryRow.h"/>
